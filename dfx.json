{
  "canisters": {
    "graphql": {
      "type": "custom",
      "build": "cargo build --target wasm32-unknown-unknown --package graphql --release",
      "candid": "src/graphql/src/graphql.did",
      "wasm": "target/wasm32-unknown-unknown/release/graphql.wasm"
    },
    "dft": {
      "type": "custom",
      "build": "cargo build --target wasm32-unknown-unknown --package  dft --release",
      "candid": "src/dft/src/token.did",
      "wasm": "target/wasm32-unknown-unknown/release/dft.wasm"
    }
  },
  "defaults": {
    "build": {
      "packtool": ""
    }
  },
  "dfx": "0.7.2",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}